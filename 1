import random

def generate_random_numbers(count, min_value, max_value):
    numbers = [random.randint(min_value, max_value) for _ in range(count)]
    with open("numbers.txt", "w") as file:
        for number in numbers:
            file.write(f"{number}\n")
    print(f"Generated {count} random numbers between {min_value} and {max_value}.")

if __name__ == "__main__":
    # Customize these values as needed
    count = 10
    min_value = 1
    max_value = 13

    generate_random_numbers(count, min_value, max_value)

